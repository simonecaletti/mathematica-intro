        SubsuperscriptBox["\[Integral]", 
         SubscriptBox["\[Theta]", "0"], "0"], 
        StyleBox["d", "TI"], 
        StyleBox["t", "TI"], "\[LongEqual]", "4", 
        SubsuperscriptBox["\[Integral]", 
