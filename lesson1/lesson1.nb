Cell["In MATHEMATICA we can solve symbolic expressions", "Text",
 CellChangeTimes->{{3.820815031534946*^9, 
  3.820815104103302*^9}},ExpressionUUID->"fd0bb6f3-855a-4bee-bc47-\
47d11c32f628"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Solve", "[", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"a", " ", 
      RowBox[{"x", "^", "2"}]}], "+", 
     RowBox[{"b", " ", "x"}], "+", "c"}], "\[Equal]", "0"}], ",", " ", "x"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.8208082918198357`*^9, 3.8208083375658197`*^9}},
 CellLabel->"In[3]:=",ExpressionUUID->"11a170ed-93d5-4bf2-9220-1c36c01b0a61"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"x", "\[Rule]", 
     FractionBox[
      RowBox[{
       RowBox[{"-", "b"}], "-", 
       SqrtBox[
        RowBox[{
         SuperscriptBox["b", "2"], "-", 
         RowBox[{"4", " ", "a", " ", "c"}]}]]}], 
      RowBox[{"2", " ", "a"}]]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", "\[Rule]", 
     FractionBox[
      RowBox[{
       RowBox[{"-", "b"}], "+", 
       SqrtBox[
        RowBox[{
         SuperscriptBox["b", "2"], "-", 
         RowBox[{"4", " ", "a", " ", "c"}]}]]}], 
      RowBox[{"2", " ", "a"}]]}], "}"}]}], "}"}]], "Output",
 CellChangeTimes->{{3.820808318481957*^9, 3.820808338989637*^9}, 
   3.823587841616929*^9, 3.853417061613467*^9},
 CellLabel->"Out[3]=",ExpressionUUID->"eb21d15e-4d31-48fe-9b92-8540a800889f"]
}, Open  ]],

Cell["and solve maths problem like ODEs", "Text",
 CellChangeTimes->{{3.82081514265215*^9, 3.820815167241737*^9}, 
   3.8208151987535877`*^9},ExpressionUUID->"a92ba126-5ee4-4e23-8cbb-\
a2cc5f6ea8cf"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"DSolve", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"x", "''"}], "[", "t", "]"}], "+", 
       RowBox[{"3", " ", 
        RowBox[{
         RowBox[{"x", "'"}], "[", "t", "]"}]}], "+", 
       RowBox[{"2", 
        RowBox[{"x", "[", "t", "]"}]}]}], "\[Equal]", "0"}], ",", " ", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"x", "[", "0", "]"}], "==", "1"}], ",", " ", 
     RowBox[{
      RowBox[{
       RowBox[{"x", "'"}], "[", "0", "]"}], "\[Equal]", "0"}]}], "}"}], ",", 
   " ", 
   RowBox[{"x", "[", "t", "]"}], ",", " ", "t"}], " ", "]"}]], "Input",
 CellChangeTimes->{{3.8208083961938763`*^9, 3.820808508081863*^9}},
 CellLabel->"In[4]:=",ExpressionUUID->"ec5f8e23-366f-4b40-bd5e-24cb9812a493"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"x", "[", "t", "]"}], "\[Rule]", 
    RowBox[{
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "2"}], " ", "t"}]], " ", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"-", "1"}], "+", 
       RowBox[{"2", " ", 
        SuperscriptBox["\[ExponentialE]", "t"]}]}], ")"}]}]}], "}"}], 
  "}"}]], "Output",
 CellChangeTimes->{{3.820808497880353*^9, 3.820808509987475*^9}, 
   3.823587841725129*^9, 3.853417061752676*^9},
 CellLabel->"Out[4]=",ExpressionUUID->"9f6dc447-cc96-474e-a034-29fbc73342a8"]
}, Open  ]],

